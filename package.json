{
  "name": "custome-pipe",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "yarn run ng serve",
    "start:aot": "yarn run serve -prod",
    "build:prod": "yarn build && yarn run copy:dist && yarn cli build -prod  --output-path \"./dist\"",
    "build:dev": "yarn build && yarn run copy:dist && yarn cli build -dev  --output-path \"./dist\"",
    "ci": "yarn run build && yarn run test && nyc report --reporter=text-lcov | coveralls",
    "deploy:builds": "ts-node ./build/deploy-build.ts",
    "deploy:development": "shipit development deploy --shipitfile ./build/shipitfile.js",
    "deploy:production": "shipit production deploy --shipitfile ./build/shipitfile.js",
    "deploy:staging": "shipit staging deploy --shipitfile ./build/shipitfile.js",
    "rollback:development": "shipit development rollback",
    "rollback:staging": "shipit staging rollback",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "engines": {
    "node": "6.11.3",
    "npm": "3.10.10"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/schematics": "0.0.34",
    "@angular/animations": "^4.2.4",
    "@angular/common": "^4.2.4",
    "@angular/compiler": "^4.2.4",
    "@angular/core": "^4.2.4",
    "@angular/forms": "^4.2.4",
    "@angular/http": "^4.2.4",
    "@angular/platform-browser": "^4.2.4",
    "@angular/platform-browser-dynamic": "^4.2.4",
    "@angular/router": "^4.2.4",
    "@types/googlemaps": "^3.30.16",
    "angular2-simple-countdown": "0.0.6",
    "angular2-text-mask": "^9.0.0",
    "angular4-carousel": "^3.1.8",
    "core-js": "^2.4.1",
    "express": "^4.16.2",
    "jquery": "^3.4.1",
    "materialize-css": "^0.100.2",
    "moment": "^2.23.0",
    "ngx-slick-carousel": "^0.4.4",
    "rxjs": "^5.4.2",
    "slick-carousel": "^1.8.1",
    "sweetalert2": "^7.29.2",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular/cli": "1.4.1",
    "@angular/compiler-cli": "^4.2.4",
    "@angular/language-service": "^4.2.4",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "^6.0.118",
    "codelyzer": "~3.1.1",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "^5.4.1",
    "shipit-cli": "^4.1.2",
    "shipit-deploy": "^4.1.2",
    "ts-node": "~3.2.0",
    "tslint": "~5.3.2",
    "typescript": "~2.3.3"
  }
}
